basic_ostream<charT, traits>::operator<< (long n)
{
	sentry ok(*this);
#ifndef _MSL_NO_EXCEPTIONS
	try
	{
#endif  // _MSL_NO_EXCEPTIONS
		if (ok)
		{
			bool failed;
